{
   "action": {
      "default_popup": "options/options.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content/prep.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "content/handlers.js", "content/configure.js", "content/iframe.js", "content/scale.js", "content/absolute.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Zoom in on a specific part of the website, like you would with a touchpad gesture!",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Mouse Pinch-To-Zoom",
   "options_ui": {
      "open_in_tab": false,
      "page": "options/options.html"
   },
   "permissions": [ "storage", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3"
}
